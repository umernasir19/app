<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_dr___iqbal_qayum"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:orientation="vertical"
        tools:context="com.example.hafizumerbinnasir.ent.Dr__Iqbal_qayum">


    <ImageView
            android:layout_width="wrap_content"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_weight="0.7 "
            android:background="@drawable/prof"
    />
    <TextView
            android:layout_width="match_parent"
            android:text="\nNAME:Dr Abdul qayum \n Qualification:MCPS \n Timing:Tuesday,Thursday,Saturday | 3:30 PM to 4:30 PM\n Location:Saifee Hospital Block F،\t \n Saifee Road، North Nazimabad،Karachi 74700"
            android:layout_height="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:textStyle="bold"
            android:layout_weight="0.4"
    />

    <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Appoint"
            android:id="@+id/call"

    />
</LinearLayout>
